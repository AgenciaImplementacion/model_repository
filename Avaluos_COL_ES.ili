MODEL Avaluos_COL_ES_V2_1 (es)
AT "mailto:vmbp@localhost"
VERSION "2017-06-02 V2.1.1"  // Inicia como V21 por provenir de la extraccion a partir del TOPIC "Avaluos" del modelo de Catastro-Registro V21 // =
  IMPORTS ISO19107_V1_MAGNABOG,Catastro_COL_ES_V2_1_6;

  DOMAIN

